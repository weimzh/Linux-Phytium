#!/usr/bin/make -f

build:
	$(MAKE) KERNELRELEASE=4.19.242-phytium ARCH=arm64 KBUILD_BUILD_VERSION=1 KBUILD_SRC=

binary-arch:
	$(MAKE) KERNELRELEASE=4.19.242-phytium ARCH=arm64 KBUILD_BUILD_VERSION=1 KBUILD_SRC= intdeb-pkg

clean:
	rm -rf debian/*tmp debian/files
	$(MAKE) clean

binary: binary-arch
